{
  "name": "can-i-send",
  "private": true,
  "version": "4.4.18",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "vite",
    "depcheck": "depcheck",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "patch": "npm version patch --no-git-tag-version && git add package.json package-lock.json && git commit -m \"chore: bump version to v$(node -p 'require(\"./package.json\").version')\"",
    "preview": "vite preview",
    "start": "npx nodemon app.js",
    "start-dev": "NODE_ENV=development npx nodemon app.js",
    "tag": "git tag -a $npm_package_version -m \"Release version $npm_package_version\" && git push origin $npm_package_version",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "firebase": "^11.5.0",
    "mailparser": "^3.7.2",
    "nodemailer": "^6.10.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hotkeys-hook": "^5.1.0",
    "react-split": "^2.0.14",
    "react-tag-input-component": "^2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.0.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^3.1.4",
    "depcheck": "^1.4.7",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.39.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.5",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
